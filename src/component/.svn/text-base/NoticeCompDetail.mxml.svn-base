<?xml version="1.0" encoding="utf-8"?>
<mx:TitleWindow xmlns:mx="http://www.adobe.com/2006/mxml" title="{resourceManager.getString('noticeView', 'detail.title')}" layout="vertical"
	width="900" height="400" showCloseButton="true" close="closeMe()">
	<mx:Metadata>
	   [ResourceBundle("noticeView")]
	</mx:Metadata>
	<mx:Script>
		<![CDATA[
			import mx.managers.PopUpManager;
			private function closeMe():void
			{
				PopUpManager.removePopUp(this);
			}
		]]>
	</mx:Script>
	<mx:VBox id="myDetail" horizontalAlign="center" width="100%" height="100%" fontSize="14"/>
</mx:TitleWindow>
