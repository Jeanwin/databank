		<plugin>
			<groupId>org.sonatype.flexmojos</groupId>
			<artifactId>flexmojos-maven-plugin</artifactId>
			<version>${flexmojos.version}</version>
			<extensions>true</extensions>
			<executions>
				<execution>
					<goals>
						<goal>wrapper</goal>
					</goals>
					<configuration>
						<outputDirectory>${basedir}/target</outputDirectory>
						<templateOutputDirectory>${basedir}/target/html-template</templateOutputDirectory>
						<templateURI>folder:${basedir}/html-template</templateURI>
						<parameters>
						  <width>100%</width>
						  <height>100%</height>
						</parameters>
						<htmlName>index</htmlName>
					  </configuration>
				</execution>
			</executions>
			 <configuration>
			    <compatibilityVersion>3.0.0</compatibilityVersion>
				<sourceFile>databank.mxml</sourceFile>
				<debug>false</debug>
				<stripModuleArtifactInfo>true</stripModuleArtifactInfo> 
				<modules>
				  <module>
					 <sourceFile>module/auth/AuthGroupFunc.mxml</sourceFile>
					 <optimize>true</optimize>
					 <finalName>AuthGroupFunc</finalName>
					 <destinationPath></destinationPath>
				  </module>
				   <module>
					 <sourceFile>module/auth/AuthUserGroupModule.mxml</sourceFile>
					 <optimize>true</optimize>
